{
  "rules": {
    "no-string-throw": true,
    "no-unused-expression": true,
    "no-duplicate-variable": true,
    "curly": true,
    "class-name": true,
    "semicolon": [
      true,
      "always"
    ],
    "triple-equals": true
  },
  "compilerOptions": {
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
